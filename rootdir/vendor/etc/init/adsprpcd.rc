# ADSP FastRPC
service adsprpcd /odm/bin/adsprpcd
   class late_start
   user media
   group media
   disabled

service audiopd /odm/bin/adsprpcd audiopd
   class main
   user media
   group media
   disabled

on property:ro.board.platform=sdm660
    enable audiopd

on property:vendor.qcom.devup=1
    start adsprpcd
